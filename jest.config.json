{
  "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
  "testEnvironment": "jsdom",
  "preset": "react-native",
  "setupFilesAfterEnv": [
    "<rootDir>/jest.setup.js"
  ],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "classes/*",
    "components/*",
    "services/*"
  ],
  "coverageReporters": ["json", "html-spa", "lcov"]
}
